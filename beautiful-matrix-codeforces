#include <iostream>
#include <cmath>
#include <cstdlib>
#define Xopt 3
#define Yopt 3
using namespace std;

int main()
{
    bool matrix[6][6];
    int x,y;

    for(int i=1;i<6;i++)
   {
        for(int j=1;j<6;j++)
    {
         cin>>matrix[i][j];

    if(matrix[i][j] == 1)
        {
                x = i;
               y = j;
    }
    }
    cout<<endl;
    }
     int moves = abs(x - Xopt) + abs(y - Yopt);
    cout<<moves;

    return 0;
}
